<div class="container">
<h1>Add a new bookmark</h1>

<%= simple_form_for [@list, @bookmark] do | f | %>
    <%= f.input :comment %>
    <%= f.association :movie, prompt: "Choose a movie"%>
    <%= f.submit "Add bookmark", class: "btn btn-primary"%>
  <% end %>

  <%= link_to "cancel", lists_path(@lists) %>

</div>
